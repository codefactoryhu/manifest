		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@tanstack/match-sorter-utils": "^8.11.8",
		"@tanstack/svelte-table": "^8.12.0",
		"@types/file-saver": "^2.0.7",
		"exceljs": "^4.4.0",
		"file-saver": "^2.0.5",
		"monaco-editor": "^0.45.0",
		"monaco-yaml": "^5.1.1",
		"svelte-select": "^5.8.3",
		"yaml": "^2.3.4"
	}
